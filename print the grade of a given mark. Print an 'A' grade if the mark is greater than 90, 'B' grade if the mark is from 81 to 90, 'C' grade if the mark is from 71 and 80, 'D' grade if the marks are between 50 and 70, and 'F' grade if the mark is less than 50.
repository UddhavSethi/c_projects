#include <stdio.h>
int main()
{
    int marks;
    scanf("%d",&marks);
    if(marks>90){
        printf("A");
    }
    if(marks<90 && marks>80){
        printf("B");
    }
    if(marks<80 && marks>70){
        printf("c");
    }
    if(marks<70 && marks>50){
        printf("D");
    }
    if(marks<50){
        printf("F");
    }
    return 0;
    
    
}
